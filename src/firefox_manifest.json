{
  "manifest_version": 2,
  "name": "Gradescope to Cal",
  "version": "0.1.0",
  "description": "Automatically sync Gradescope assignments to Google Calendar",
  
  "permissions": [
    "storage",
    "*://*.gradescope.com/*"
  ],
  
  "content_scripts": [
    {
      "matches": [
        "*://*.gradescope.com/*"
      ],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ],
  
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Gradescope to Cal",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },

  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  
  "web_accessible_resources": []
}